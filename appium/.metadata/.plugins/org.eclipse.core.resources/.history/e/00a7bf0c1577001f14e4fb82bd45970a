package tests;

import org.openqa.selenium.remote.DesiredCapabilities;
import java.net.*;

//import com.google.common.collect.ImmutableMap;

import io.appium.java_client.MobileElement;
import io.appium.java_client.android.AndroidDriver;
import io.appium.java_client.remote.MobileCapabilityType;
import io.appium.java_client.remote.MobilePlatform;

public class Baseclass{
	public static void main(String[] args) throws MalformedURLException {
		DesiredCapabilities cap=new DesiredCapabilities();
		cap.setCapability(MobileCapabilityType.PLATFORM_NAME,MobilePlatform.ANDROID);
		cap.setCapability(MobileCapabilityType.DEVICE_NAME, "Pixel 2");
		cap.setCapability("automationName", "uiautomator2");
		cap.setCapability("ignoreHiddenApiPolicyError", true);
		cap.setCapability("appPackage","com.example.fooddeliveryapp");
		cap.setCapability("appActivity","com.example.fooddeliveryapp.MainActivity");
	
		AndroidDriver<MobileElement> driver=new AndroidDriver<MobileElement>(new URL("http://127.0.0.1:4723"),cap);
		
		driver.quit();
	}
}